{
    "name": "tms-shell",
    "version": "1.0.0",
    "description": "shell app for min-side",
    "main": "index.js",
    "scripts": {
        "start": "rollup -c rollup.config.dev.js -w",
        "build": "rollup -c rollup.config.prod.js --file dist/bundle.js --format esm",
        "dev-express": "nodemon server/server.js",
        "start-express": "node server/server.js",
        "prepare": "husky install"
    },
    "author": "navikt",
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "7.13.10",
        "@babel/preset-react": "7.12.13",
        "@eik/rollup-plugin": "4.0.49",
        "@rollup/plugin-babel": "5.3.0",
        "@rollup/plugin-commonjs": "17.1.0",
        "@rollup/plugin-json": "4.1.0",
        "@rollup/plugin-node-resolve": "11.2.0",
        "@rollup/plugin-replace": "2.4.1",
        "babel": "6.23.0",
        "husky": "7.0.0",
        "msw": "0.28.0",
        "nodemon": "2.0.15",
        "pretty-quick": "3.1.3",
        "rollup": "2.41.1",
        "rollup-plugin-copy": "3.4.0",
        "rollup-plugin-livereload": "2.0.0",
        "rollup-plugin-postcss": "4.0.2",
        "rollup-plugin-postcss-webpack-alias-less-loader": "1.0.0",
        "rollup-plugin-serve": "1.1.0"
    },
    "dependencies": {
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-router-dom": "5.2.0"
    },
    "prettier": {
        "trailingComma": "es5",
        "tabWidth": 4,
        "semi": true,
        "singleQuote": true,
        "printWidth": 120
    },
    "msw": {
        "workerDirectory": "public"
    }
}
